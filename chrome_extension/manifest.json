{
  "name": "Fosterzen Bookmarks Sync",
  "version": "3.0",
  "description": ":Fosterzen Bookmarks Sync:",
  
  "permissions": [
    "bookmarks","tabs","http://*/"
  ],
  "browser_action": {
      "default_title": "Fosterzen Bookmarks Sync",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com  https://ssl.google-analytics.com;  object-src 'self'"
}
